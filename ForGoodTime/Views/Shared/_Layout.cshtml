<!DOCTYPE html>
<html>
<head> 
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ForGoodTime</title>   
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true">
        <!-- animate CSS -->
        <link rel="stylesheet" href="~/css/animate.css" asp-append-version="true">
        <!-- owl carousel CSS -->
        <link rel="stylesheet" href="~/css/owl.carousel.min.css" asp-append-version="true">
        <!-- nice select CSS -->
        <link rel="stylesheet" href="~/css/nice-select.css" asp-append-version="true">
        <!-- font awesome CSS -->
        <link rel="stylesheet" href="~/css/all.css" asp-append-version="true">
        <!-- flaticon CSS -->
        <link rel="stylesheet" href="~/css/flaticon.css" asp-append-version="true">
        <link rel="stylesheet" href="~/css/themify-icons.css" asp-append-version="true">
        <link rel="stylesheet" href="~/css/lightslider.min.css" asp-append-version="true">
        <!-- font awesome CSS -->
        <link rel="stylesheet" href="~/css/magnific-popup.css" asp-append-version="true">
        <!-- swiper CSS -->
        <link rel="stylesheet" href="~/css/slick.css" asp-append-version="true">
        <link rel="stylesheet" href="~/css/price_rangs.css" asp-append-version="true">
        <!-- style CSS -->
        <link rel="stylesheet" href="~/css/style.css" asp-append-version="true">
        <link href="~/css/rating.css" rel="stylesheet" asp-append-version="true" />   
</head>
<body>
    <header class="main_menu home_menu">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <a asp-controller="Home" asp-action="Index" class="navbar-brand"> <img src="@*img/logo.png*@" alt="Mebelyer"> </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse"
                                data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                                aria-expanded="false" aria-label="Toggle navigation">
                            <span class="menu_icon"><i class="fas fa-bars"></i></span>
                        </button>
                        <div class="collapse navbar-collapse main-menu-item" id="navbarSupportedContent">
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a asp-controller="Home" asp-action="Index" class="nav-link">Home</a>
                                </li>

                                @await Component.InvokeAsync("CategorySummary")

                                <li class="nav-item">
                                    <a asp-controller="Home" asp-action="Contact" class="nav-link">Contact</a>
                                </li>
                                @await Html.PartialAsync("_LoginPartial")
                            </ul>
                        </div>
                        <div class="hearer_icon d-flex">
                            <a id="search_1" href="javascript:void(0)"><i class="ti-search"></i></a>
                            <div class="dropdown heart">

                                @await Component.InvokeAsync("WishListSummary")

                            </div>

                            <div class="dropdown cart">
                                @await Component.InvokeAsync("ShoppingCartSummary")
                                <!-- <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    <div class="single_product">
                                    </div>
                                </div> -->
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
        <div class="search_input" id="search_input_box">
            <div class="container ">
                <form class="d-flex justify-content-between search-inner">
                    <input type="text" class="form-control" id="search_input" placeholder="Search Here">
                    <button type="submit" class="btn"></button>
                    <span class="ti-close" id="close_search" title="Close Search"></span>
                </form>
            </div>
        </div>
    </header>
  
    @RenderBody()
    <partial name="_CookieConsentPartial" />
    <!--::footer_part start::-->
    <footer class="footer_part">
        <div class="container">
            <div class="row justify-content-around">
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h4>Top Products</h4>
                        <ul class="list-unstyled">
                            <li><a href="">Managed Website</a></li>
                            <li><a href="">Manage Reputation</a></li>
                            <li><a href="">Power Tools</a></li>
                            <li><a href="">Marketing Service</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h4>Quick Links</h4>
                        <ul class="list-unstyled">
                            <li><a href="">Jobs</a></li>
                            <li><a href="">Brand Assets</a></li>
                            <li><a href="">Investor Relations</a></li>
                            <li><a href="">Terms of Service</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h4>Features</h4>
                        <ul class="list-unstyled">
                            <li><a href="">Jobs</a></li>
                            <li><a href="">Brand Assets</a></li>
                            <li><a href="">Investor Relations</a></li>
                            <li><a href="">Terms of Service</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h4>Resources</h4>
                        <ul class="list-unstyled">
                            <li><a href="">Guides</a></li>
                            <li><a href="">Research</a></li>
                            <li><a href="">Experts</a></li>
                            <li><a href="">Agencies</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-4">
                    <div class="single_footer_part">
                        <h4>Newsletter</h4>
                        <p>
                            Heaven fruitful doesn't over lesser in days. Appear creeping
                        </p>
                        <div id="mc_embed_signup">
                            <form target="_blank"
                                  action="https://spondonit.us12.list-manage.com/subscribe/post?u=1462626880ade1ac87bd9c93a&amp;id=92a4423d01"
                                  method="get" class="subscribe_form relative mail_part">
                                <input type="email" name="email" id="newsletter-form-email" placeholder="Email Address"
                                       class="placeholder hide-on-focus" onfocus="this.placeholder = ''"
                                       onblur="this.placeholder = ' Email Address '">
                                <button type="submit" name="submit" id="newsletter-submit"
                                        class="email_icon newsletter-submit button-contactForm">
                                    subscribe
                                </button>
                                <div class="mt-10 info"></div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

        </div>

    </footer>

    <!-- jquery plugins here-->
    <script src="~/js/jquery-1.12.1.min.js" asp-append-version="true"></script>
    <!-- popper js -->
    <script src="~/js/popper.min.js" asp-append-version="true"></script>

    <script src="~/js/bootstrap.min.js" asp-append-version="true"></script>

    <script src="~/js/jquery.magnific-popup.js" type="text/javascript" asp-append-version="true"></script>

    <script src="~/js/swiper.min.js" asp-append-version="true"></script>

    <script src="~/js/masonry.pkgd.js" asp-append-version="true"></script>
    <script src="~/js/masonry.pkgd.min.js" asp-append-version="true"></script>

    <script src="~/js/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.nice-select.min.js" asp-append-version="true" type="text/javascript"></script>

    <script src="~/js/slick.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.counterup.min.js" asp-append-version="true"></script>
    <script src="~/js/waypoints.min.js" asp-append-version="true"></script>
    <script src="~/js/contact.js" asp-append-version="true"></script>
    <script src="~/js/jquery.ajaxchimp.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.form.js" asp-append-version="true"></script>
    <script src="~/js/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="~/js/mail-script.js" asp-append-version="true"></script>
    <script src="~/js/stellar.js" asp-append-version="true"></script>
    <script src="~/js/price_rangs.js" asp-append-version="true"></script>
    <script src="~/js/lightslider.min.js" asp-append-version="true"></script>
    <script src="~/js/custom.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
